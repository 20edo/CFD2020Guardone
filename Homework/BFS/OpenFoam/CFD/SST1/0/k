FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.45;

boundaryField
{
    BACK
    {
        type            empty;
    }
    WALL
    {
        type            fixedValue;
        value           uniform 0;
    }
    INLET
    {
        type            fixedValue;
        value           uniform 0.45;
    }
    FRONT
    {
        type            empty;
    }
    OUTLET
    {
        type            inletOutlet;
        inletValue      uniform 0.45;
        value           uniform 0;
    }
}


