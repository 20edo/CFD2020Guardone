FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 2800;

boundaryField
{
    BACK
    {
        type            empty;
    }
    WALL
    {
        type            zeroGradient;
    }
    INLET
    {
        type            fixedValue;
        value           uniform 2800;
    }
    FRONT
    {
        type            empty;
    }
    OUTLET
    {
        type            inletOutlet;
        inletValue      uniform 2800;
        value           uniform 2800;
    }
}


